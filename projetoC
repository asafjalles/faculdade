#include <stdio.h>

int main() {
	int vetor[3], vetor1, vetor2, vetor3;

	printf("Informe o primeiro Vetor: ");
	scanf("%d", & vetor1);

	printf("Informe o Segundo Vetor: ");
	scanf("%d", & vetor2);

	printf("Informe o Terceiro Vetor: ");
	scanf("%d", & vetor3);

	printf("Vetor[1] = %d \n", vetor1);
	printf("Vetor[2] = %d \n", vetor2);
	printf("Vetor[3] = %d \n", vetor3);	

	return 0;
}

